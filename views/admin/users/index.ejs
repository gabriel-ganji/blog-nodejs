<%- include("../../partials/header.ejs") %>
<body>
    <%- include("../../partials/navbar.ejs") %>
    <div class="container">
        <br>
        <h2>Usuários</h2><br>
        <a class="btn btn-success" href="/admin/users/create">Criar novo usuário</a>
        <br><br>
        <table class="table table-bordered whiteTable">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
               
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.id %></td>
                        <td><%= user.email %></td>
                    </tr>
                    
                <% }) %>
            </tbody>
        </table>
    </div>
   
</body>

<%- include("../../partials/footer.ejs") %>